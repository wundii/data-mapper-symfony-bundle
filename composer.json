{
  "name": "wundii/data-mapper-symfony-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Bundle for Wundii Data Mapper",
  "keywords": ["symfony","symfony-bundle","bundle","datamapper","mapper","xml","json","array","object", "deserialization", "deserialize"],
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wunderwald",
      "email": "wundii@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "ext-pcre": "*",
    "ext-reflection": "*",
    "ext-simplexml": "*",
    "ext-spl": "*",
    "ext-xml": "*",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/http-kernel": "^6.4|^7.0",
    "wundii/data-mapper": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Wundii\\DataMapper\\SymfonyBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Wundii\\DataMapper\\SymfonyBundle\\Tests\\": "Tests/"
    }
  },
  "minimum-stability": "dev"
}